        android:layout_marginTop="8dp"
        android:backgroundTint="#472828"
        android:text="95/A"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/en_yakin_kalkis_35E2_2" />

    <TextView
        android:id="@+id/en_yakin_kalkis_95A_1"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="Kalkış bilgileri yükleniyor..."
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buton_95A" />

    <TextView
        android:id="@+id/en_yakin_kalkis_95A_2"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="Kalkış bilgileri yükleniyor..."
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/en_yakin_kalkis_95A_1" />

    <Button
        android:id="@+id/buton_43H"
        android:layout_width="330dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#472828"
        android:text="43/H"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/en_yakin_kalkis_95A_2" />

    <TextView
        android:id="@+id/en_yakin_kalkis_43H_1"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="Kalkış bilgileri yükleniyor..."
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buton_43H" />

    <TextView
        android:id="@+id/en_yakin_kalkis_43H_2"
        android:layout_width="330dp"
        android:layout_height="wrap_content"
        android:text="Kalkış bilgileri yükleniyor..."
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/en_yakin_kalkis_43H_1" />

</androidx.constraintlayout.widget.ConstraintLayout>